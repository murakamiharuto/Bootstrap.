{
  "name": "ng2-bootstrap",
  "version": "1.1.16",
  "description": "angular2 bootstrap components",
  "scripts": {
    "flow.docs": "typedoc -- --exclude '**/*.spec.ts' ./src/",
    "flow.compile": "run-s flow.compile:common flow.compile:system",
    "build": "ngc -p ./src",
    "flow.compile:common": "ngc -p ./src/tsconfig.publish.json",
    "flow.compile:system": "webpack -p",
    "flow.copy:src": "cpy package.json dist/package",
    "flow.clean": "del-cli dist coverage",
    "flow.deploy:gh-pages": "npm run flow.build:prod && gh-pages -d demo-build",
    "flow.lint": "tslint \"**/*.ts\" -c tslint.json -e \"node_modules/**\" -e \"dist/**\"",
    "flow.changelog": "conventional-changelog -i CHANGELOG.md -s -p angular -v",
    "flow.github-release": "conventional-github-releaser -p angular",
    "flow.build:prod": "ng build --prod",
    "flow.build:dev": "ng build",
    "flow.serve:dev": "ng serve",
    "flow.serve:prod": "ng serve --prod",
    "prepublish": "echo 'use make.js publish insteed' && exit 1",
    "postpublish": "npm run flow.deploy:gh-pages",
    "start": "ng serve",
    "pretest": "npm run flow.lint",
    "test": "cross-env NODE_ENV=test karma start",
    "test:watch": "cross-env NODE_ENV=test karma start --auto-watch --no-single-run",
    "preversion": "npm test",
    "version": "npm run flow.changelog && git add -A",
    "postversion": "git push origin development && git push --tags && npm run flow.copy:src",
    "pree2e": "webdriver-manager update"
  },
  "main": "bundles/ng2-bootstrap.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "angular2",
    "bootstrap",
    "angularjs",
    "twitter-bootstrap"
  ],
  "author": "Dmitriy Shekhovtsov <valorkin@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/valor-software/ng2-bootstrap.git"
  },
  "bugs": {
    "url": "https://github.com/valor-software/ng2-bootstrap/issues"
  },
  "homepage": "https://github.com/valor-software/ng2-bootstrap#readme",
  "dependencies": {
    "moment": "2.15.1"
  },
  "peerDependencies": {
    "@angular/common": "^2.0.0",
    "@angular/compiler": "^2.0.0",
    "@angular/core": "^2.0.0",
    "@angular/forms": "^2.0.0"
  },
  "devDependencies": {
    "@angular/common": "2.1.2",
    "@angular/compiler": "2.1.2",
    "@angular/compiler-cli": "2.1.2",
    "@angular/core": "2.1.2",
    "@angular/forms": "2.1.2",
    "@angular/platform-browser": "2.1.2",
    "@angular/platform-browser-dynamic": "2.1.2",
    "@angular/platform-server": "2.1.2",
    "@angular/router": "3.1.2",
    "@types/jasmine": "2.5.35",
    "@types/node": "6.0.42",
    "@types/webpack": "1.12.35",
    "angular-cli": "1.0.0-beta.19-3",
    "async": "2.1.1",
    "bootstrap": "3.3.7",
    "codecov": "1.0.1",
    "conventional-changelog-cli": "1.2.0",
    "conventional-github-releaser": "1.1.3",
    "core-js": "2.4.1",
    "cpy-cli": "1.0.1",
    "cross-env": "3.1.2",
    "del-cli": "0.2.0",
    "gitignore-to-glob": "0.2.1",
    "google-code-prettify": "1.0.5",
    "html-loader": "0.4.4",
    "jasmine": "2.5.2",
    "jasmine-core": "2.4.1",
    "jasmine-data-provider": "2.2.0",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "lite-server": "2.2.2",
    "markdown-loader": "github:valorkin/markdown-loader",
    "marked": "0.3.6",
    "npm-run-all": "3.1.1",
    "pre-commit": "1.1.3",
    "protractor": "4.0.9",
    "reflect-metadata": "0.1.8",
    "require-dir": "0.3.1",
    "rxjs": "5.0.0-beta.12",
    "systemjs-builder": "0.15.32",
    "ts-helpers": "^1.1.1",
    "ts-node": "1.2.1",
    "tslint-config-valorsoft": "1.1.3",
    "typedoc": "0.5.0",
    "typescript": "2.0.8",
    "wallaby-webpack": "0.0.26",
    "webdriver-manager": "10.2.5",
    "zone.js": "0.6.25"
  }
}
